{
  "name": "process-looker",
  "version": "1.0.0",
  "description": "get details of process",
  "main": "app.js",
  "dependencies": {
    "bootstrap": "3.3.7",
    "child_process": "1.0.2",
    "electron": "1.8.1",
    "electron-packager": "10.1.1",
    "jquery": "3.2.1",
    "util": "0.10.3"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . process-looker --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assests/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Process Looker\"",
    "package-linux": "electron-packager . process-looker --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bapinmalakar/process-looker.git"
  },
  "author": "biplab malakar",
